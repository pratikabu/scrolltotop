{
	"manifest_version": 2,
	"name": "Scroll To Top",
	"version": "4.7",
	
	"description": "Scroll to top and vice versa in a window.",
	"icons": {
		"16": "icons/pratikabu-stt-16.png",
		"48": "icons/pratikabu-stt-48-1.png",
		"128": "icons/pratikabu-stt-128.png"
	},
	
	"author": "Pratik Soni",
	"background": {
		"scripts": [ "background.js" ],
		"persistent": false
	},
	
	"content_scripts":[
		{
			"css":[ "pratikabu-stt.css" ],
			"matches":[ "<all_urls>" ],
			"js":[
				"thirdparty/pratikabu-jquery-3.2.1.min.js",
				"thirdparty/pratikabu-jquery-rotate.js",
				"browserspecific/pratikabu-stt-impl.js",
				"pratikabu-stt.js"
			]
		}
	],
	"permissions": [
		"storage"
	],
	"homepage_url": "http://pratikabu.users.sourceforge.net/extensions/scrolltotop",
	"options_ui": {
		"page": "options/options.html",
		"open_in_tab": true
	},
